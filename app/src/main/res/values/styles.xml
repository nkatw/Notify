<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="BasicButton">
		<item name="android:background">?attr/selectableItemBackgroundBorderless</item>
	</style>
	<style name="NotificationButtonBase">
		<item name="android:background">?android:selectableItemBackground</item>
		<item name="android:layout_width">46dip</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:scaleType">fitCenter</item>
		<item name="android:padding">2dip</item>
	</style>
	<style name="NotificationButton" parent="NotificationButtonBase">
	</style>

	<style name="NotificationButton.ExpandedBase">
		<item name="android:background">?android:selectableItemBackground</item>
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:layout_weight">1</item>
		<item name="android:layout_gravity">center</item>
		<item name="android:scaleType">fitCenter</item>
	</style>

	<style name="NotificationButton.Expanded" parent="@style/NotificationButton.ExpandedBase">
		<item name="android:padding">10dip</item>
	</style>

	<style name="NotificationText.Title" parent="@android:style/TextAppearance.StatusBar.EventContent.Title"></style>
	<style name="NotificationText.Content" parent="@android:style/TextAppearance.StatusBar.EventContent"></style>

	<style name="NotificationLayoutDivider">
		<item name="android:divider">?android:listDivider</item>
	</style>
	<style name="NotificationDivider">
		<item name="android:background">?android:dividerHorizontal</item>
	</style>

	<style name="MoreButton" parent="@style/BasicButton">
		<item name="android:paddingRight">2dp</item>
	</style>

    <style name="PlaybackControl" parent="@style/BasicButton">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_marginLeft">4dip</item>
        <item name="android:layout_marginRight">4dip</item>
        <item name="android:layout_width">@dimen/Button</item>
        <item name="android:layout_height">@dimen/Button</item>
        <item name="android:contentDescription">@null</item>
    </style>

	<style name="PlaybackControl.Small" parent="@style/PlaybackControl">
        <item name="android:layout_width">@dimen/Button.Small</item>
        <item name="android:layout_height">@dimen/Button.Small</item>
    </style>

	<style name="PlaybackControl.Large" parent="@style/PlaybackControl">
		<item name="android:layout_width">@dimen/Button.Large</item>
		<item name="android:layout_height">@dimen/Button.Large</item>
		<item name="android:paddingLeft">3dp</item>
		<item name="android:paddingRight">3dp</item>
	</style>

	<style name="PlaybackControl.Match" parent="@style/PlaybackControl">
		<item name="android:layout_height">match_parent</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:scaleType">fitCenter</item>
	</style>

	<style name="PlaybackControl.BottomBar" parent="@style/PlaybackControl.Match">
		<item name="android:layout_width">0dp</item>
		<item name="android:layout_weight">1</item>
	</style>

	<style name="DownloadActionButton" parent="@style/BasicButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textSize">22sp</item>
		<item name="android:paddingTop">4dip</item>
		<item name="android:paddingBottom">4dip</item>
		<item name="android:paddingRight">4dip</item>
		<item name="android:paddingLeft">4dip</item>
	</style>

	<style name="DownloadActionImageButton" parent="@style/BasicButton">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingTop">8dip</item>
		<item name="android:paddingBottom">8dip</item>
		<item name="android:paddingRight">16dip</item>
		<item name="android:paddingLeft">16dip</item>
	</style>

	<style name="NotifyRootLayoutStyle">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">match_parent</item>
		<item name="android:background">@color/notifyBackgroundColor</item>
		<item name="android:orientation">vertical</item>
		<item name="android:paddingStart">165px</item>
		<item name="android:paddingEnd">165px</item>
	</style>

	<style name="NotifyMainPageTitle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">56px</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
		<item name="android:textSize">38px</item>
		<item name="android:textStyle">bold</item>
		<item name="android:layout_marginBottom">50px</item>
		<item name="android:text">Title String</item>
	</style>

	<style name="NotifyMainPageTitle.genres" parent="NotifyMainPageTitle">
		<item name="android:layout_marginBottom">0px</item>
		<item name="android:layout_alignParentLeft">true</item>
		<item name="android:layout_centerVertical">true</item>
		<item name="android:text">Title String</item>
	</style>

	<style name="NotifyMainPageBlock">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:orientation">horizontal</item>
		<item name="android:paddingBottom">@dimen/NotifyMainPage.Block.BottomMargin</item>
	</style>

	<style name="NotifyMainPageCoverArt">
		<item name="android:layout_width">290px</item>
		<item name="android:layout_height">290px</item>
		<item name="android:layout_alignParentTop">true</item>
		<item name="android:background">@color/notifyUndefinedAlbumsDefaultColor</item>
	</style>

	<style name="NotifyMainPageButtonStyle">
		<item name="android:layout_width">290px</item>
		<item name="android:layout_height">356px</item>
		<item name="android:layout_marginRight">36.7px</item>
	</style>

    <style name="NotifyMainPageSongButtonTitle">
        <item name="android:layout_width">290px</item>
        <item name="android:layout_height">41px</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/notifyFontColorWhite</item>
        <item name="android:textSize">28px</item>
		<item name="android:gravity">center</item>
		<item name="android:text">Song Name Text Song Name Text</item>
    </style>

	<style name="NotifyMainPageAlbumButtonTitle" parent="NotifyMainPageSongButtonTitle">
		<item name="android:text">Album Name Text Album Name Text</item>
	</style>

	<style name="NotifyMainPageGenresButton">
		<item name="android:layout_width">300px</item>
		<item name="android:layout_height">300px</item>
		<item name="android:layout_marginRight">23px</item>
		<item name="android:layout_marginBottom">35px</item>
	</style>

	<style name="NotifyMainPageGenresButtonImage">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">match_parent</item>
	</style>

	<style name="NotifyMainPageGenresButtonTitle">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">match_parent</item>
		<item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">@color/notifyFontColorWhite</item>
        <item name="android:textSize">35px</item>
		<item name="android:text">Genre Name</item>
	</style>

	<style name="NotifyToolbarButton">
		<item name="android:layout_height">match_parent</item>
		<item name="android:background">@color/notifyBackgroundColor</item>\
		<item name="android:layout_marginStart">73px</item>
		<item name="android:scaleType">fitCenter</item>
	</style>

	<style name="NotifyToolbarButton.back">
		<item name="android:layout_marginStart">0dp</item>
	</style>

	<style name="NotifyArtistPageAlbumItemLayout">
		<item name="android:layout_width">290px</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginEnd">36px</item>
		<item name="android:layout_marginBottom">100px</item>
		<item name="android:orientation">vertical</item>
	</style>

	<style name="NotifyArtistPageAlbumItemImage">
		<item name="android:layout_width">290px</item>
		<item name="android:layout_height">290px</item>
		<item name="android:layout_marginBottom">30px</item>
		<item name="android:background">@color/notifyUndefinedAlbumsDefaultColor</item>
	</style>

	<style name="NotifyArtistPageAlbumItemName">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginBottom">15px</item>
		<item name="android:singleLine">true</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
		<item name="android:textSize">30px</item>
		<item name="android:textAllCaps">false</item>
	</style>

	<style name="NotifyArtistPageAlbumItemYear">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:gravity">start</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
		<item name="android:textSize">25px</item>
		<item name="android:textAllCaps">false</item>
	</style>

	<style name="NotifyArtistAndAlbumPageTitleLayout">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginBottom">150px</item>
	</style>

	<style name="NotifyArtistAndAlbumPageTitle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginBottom">80px</item>
		<item name="android:textAllCaps">false</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
	</style>

	<style name="NotifyArtistAndAlbumPageTitle.artist">
		<item name="android:textStyle">bold</item>
		<item name="android:textSize">60px</item>
	</style>

	<style name="NotifyArtistAndAlbumPageTitle.album">
		<item name="android:textSize">35px</item>
	</style>

	<style name="NotifyArtistAndAlbumPageSubtitle">
		<item name="android:lineSpacingExtra">4.5px</item>
		<item name="android:textAllCaps">false</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
	</style>

	<style name="NotifyArtistAndAlbumPageSubtitle.artist">
		<item name="android:letterSpacing">0.1</item>
		<item name="android:layout_width">785px</item>
		<item name="android:layout_height">180px</item>
		<item name="android:textSize">30px</item>
	</style>

	<style name="NotifyArtistAndAlbumPageSubtitle.album">
		<item name="android:letterSpacing">0.091</item>
		<item name="android:layout_width">825px</item>
		<item name="android:layout_height">235px</item>
		<item name="android:textSize">55px</item>
		<item name="android:textStyle">bold</item>
		<item name="android:lineSpacingExtra">17px</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">3</item>
	</style>

	<style name="NotifyArtistAndAlbumPageTitleImage">
		<item name="android:layout_width">380px</item>
		<item name="android:layout_height">380px</item>
		<item name="android:src">@color/notifyUndefinedAlbumsDefaultColor</item>
	</style>

	<style name="NotifyCategoryLayout">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginBottom">70px</item>
		<item name="android:orientation">vertical</item>
	</style>

	<style name="NotifyCategoryTitle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
		<item name="android:textSize">@dimen/Notify.categoryTitle.fontSize</item>
	</style>

	<style name="NotifyCategoryLine">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">3px</item>
		<item name="android:src">@drawable/notify_gradient_color</item>
	</style>

	<style name="NotifyPlayerPageTimeText">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">45px</item>
		<item name="android:textColor">@color/notifyFontColorWhite</item>
		<item name="android:textSize">@dimen/Notify.playerPage.fontSize</item>
	</style>
</resources>